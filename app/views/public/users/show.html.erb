   <div class="container pb-5">
      <div class="col-11 mx-auto">
         <h3><strong>マイページ</strong></h3>
       </div>

         </br>
         </br>

     <div class="row"></div>
        <div class="col-11">
          <h4><strong>登録情報</strong></h4>
        </div>



    <div class="col-7 sm-3">
      <table class="table table-bordered">
        <thead>
          <tr>
            <th>氏名</th>
            <th><%= @user.last_name %><%= @user.first_name %></th>
          </tr>
        </thead>

        <tbody>
          <tr>
            <th>カナ</th>
            <th><%= @user.last_name_kana %><%= @user.first_name_kana %></th>
          </tr>

          <tr>
            <th>所属</th>
            <th><%= @user.department %></th>
          </tr>

          <tr>
            <th>役職</th>
            <th><%= @user.post %></th>
          </tr>

          <tr>
            <th>電話番号</th>
            <th><%= @user.phone_number %></th>
          </tr>

          <tr>
            <th>メールアドレス</th>
            <th><%= @user.email %></th>
          </tr>
     　　</tbody>
     </table>
    </div>
       </br>



     <div class="row"></div>
        <div class="col-11">
          <h4>連絡先 <%= link_to  "一覧をみる", contacts_path, class:"btn btn-outline-danger ml-5" %></h4>
        </div>
        </br>
     <div class="row"></div>
        <div class="col-11">
          <h4>コール履歴<%= link_to  "コール履歴", call_histories_path, class:"btn btn-outline-danger ml-4" %></h4>
        </div>
    </div>

    </br>
